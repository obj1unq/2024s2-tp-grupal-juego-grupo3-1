import wollok.game.*
import inicio.*
import reloj.*


program aventuraDelMate {

	game.title("Aventura del Mate")
	game.height(12)
	game.width(20)
	game.cellSize(50)
	game.boardGround("boardGame.png")
    game.start()
	
	game.schedule(0, { game.sound("musiJuego.mp3").play() })
	
	inicio.ejecutarInicio()
		
	game.onTick(1000, "reloj", {reloj.tick()})

	
}
 

